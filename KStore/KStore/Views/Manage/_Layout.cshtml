@{ 
    Layout = "/Views/Shared/_Layout.cshtml";
}


<div class="breadcrumbs">
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <ul>
                    <li class="home"> <a title="Go to Home Page" href="/">Home</a><span>&raquo;</span></li>
                    <li><strong>Manage your account</strong></li>
                </ul>
            </div>
        </div>
    </div>
</div>

<div class="breadcrumbs">
